{
    "name": "antigravity-focus",
    "displayName": "Antigravity Focus Mode",
    "description": "Keeps the main window lightweight and resource-efficient for Antigravity",
    "version": "1.1.0",
    "publisher": "devin",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "antigravityFocus.toggle",
                "title": "Antigravity Focus: Toggle Focus Mode"
            },
            {
                "command": "antigravityFocus.cleanWindow",
                "title": "Antigravity Focus: Clean Main Window"
            },
            {
                "command": "antigravityFocus.resetLayout",
                "title": "Antigravity Focus: Reset Layout to Normal"
            }
        ],
        "keybindings": [
            {
                "command": "antigravityFocus.toggle",
                "key": "ctrl+shift+f12"
            },
            {
                "command": "antigravityFocus.cleanWindow",
                "key": "ctrl+shift+a"
            }
        ],
        "configuration": {
            "title": "Antigravity Focus",
            "properties": {
                "antigravityFocus.enabled": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enable automatic tab closing and resource saving"
                }
            }
        }
    }
}